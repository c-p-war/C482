<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wardlaw.mainscreen.ModifyPartController">
    <children>
        <Label fx:id="ModifyPartLabel" layoutX="14.0" layoutY="27.0" text="Modify Part" />
        <RadioButton fx:id="ModifyPartRadioInHouse" layoutX="99.0" layoutY="27.0" mnemonicParsing="false" onAction="#modifyPartInHouseRadioClicked" selected="true" text="In-House">
