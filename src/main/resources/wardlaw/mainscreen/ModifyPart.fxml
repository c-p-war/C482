            <ToggleGroup fx:id="modifyPartToggleGroup" />
         </toggleGroup></RadioButton>
        <RadioButton fx:id="ModifyPartRadioOutsourced" layoutX="190.0" layoutY="27.0" mnemonicParsing="false" onAction="#modifyPartOutsourcedRadioClicked" text="Outsourced" toggleGroup="$modifyPartToggleGroup" />
        <Label fx:id="ModifyPartLabelID" layoutX="21.0" layoutY="119.0" text="ID" />
        <Label fx:id="ModifyPartLabelName" layoutX="21.0" layoutY="153.0" text="Name" />
        <Label fx:id="ModifyPartLabelInv" layoutX="21.0" layoutY="187.0" text="Inv" />
        <Label fx:id="ModifyPartLabelPrice" layoutX="21.0" layoutY="221.0" text="Price/Cost" />
        <Label fx:id="ModifyPartLabelMax" layoutX="21.0" layoutY="255.0" text="Max" />
        <Label fx:id="ModifyPartLabelDynamic" layoutX="21.0" layoutY="289.0" text="Machine ID" />
        <TextField fx:id="ModifyPartTxtFieldId" disable="true" editable="false" layoutX="121.0" layoutY="110.0" />
        <TextField fx:id="ModifyPartTxtFieldName" layoutX="121.0" layoutY="144.0" />
        <TextField fx:id="ModifyPartTxtFieldInv" layoutX="121.0" layoutY="178.0" />
        <TextField fx:id="ModifyPartTxtFieldPrice" layoutX="121.0" layoutY="212.0" />
        <TextField fx:id="ModifyPartTxtFieldMax" layoutX="121.0" layoutY="246.0" />
        <TextField fx:id="ModifyPartTxtFieldDynamic" layoutX="121.0" layoutY="280.0" />
        <Label fx:id="ModifyPartLabelMin" layoutX="306.0" layoutY="250.0" text="Min" />
        <TextField fx:id="ModifyPartTxtFieldMin" layoutX="366.0" layoutY="247.0" />
        <Button fx:id="ModifyPartBtnSave" layoutX="261.0" layoutY="338.0" mnemonicParsing="false" onAction="#modifyPartSaveBtnClicked" text="Save" />
        <Button fx:id="ModifyPartBtnCancel" layoutX="338.0" layoutY="339.0" mnemonicParsing="false" onAction="#modifyPartCancelBtnClicked" text="Cancel" />
    </children>
</AnchorPane>
